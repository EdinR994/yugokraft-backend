  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main --max-old-space-size=4096",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "knex:migrate:make": "knex --knexfile ./database/knexfile.js migrate:make",
    "knex:migrate:latest": "knex --knexfile ./database/knexfile.js migrate:latest",
    "knex:migrate:list": "knex --knexfile ./database/knexfile.js migrate:list",
    "knex:migrate:rollback": "knex --knexfile ./database/knexfile.js migrate:rollback",
    "knex:migrate:up": "knex --knexfile ./database/knexfile.js migrate:up",
    "knex:migrate:down": "knex --knexfile ./database/knexfile.js migrate:down"
  },

    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-sendgrid": "^1.0.0",
    "@types/pino": "^6.3.4",
    "adm-zip": "^0.5.1",
